{
  "name": "studio",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "serve": "vite --port=3000",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:dev": "vue-tsc --noEmit --skipLibCheck && vite build --mode development",
    "format": "npx prettier --write \"./*.{js,ts,vue,json}\" \"./src/**/*.{js,ts,vue,json}\"",
    "serve:localhost": "vite --mode localhost --port=3000",
    "start": "vite preview",
    "i18n": "node ../dashboard/src/i18n/scan.js",
    "genql": "genql --endpoint https://upstage.live/api/studio_graphql/ --output ./src/genql/studio && genql --endpoint https://upstage.live/api/config_graphql/ --output ./src/genql/config"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@apollo/client": "^3.8.8",
    "@tiptap/extension-image": "^2.1.13",
    "@tiptap/extension-link": "^2.1.13",
    "@tiptap/extension-placeholder": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "@tiptap/vue-3": "^2.1.13",
    "@vue/apollo-composable": "^4.0.0-beta.12",
    "@vueuse/core": "^6.7.3",
    "ant-design-vue": "^4.0.3",
    "autoprefixer": "^10.3.7",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "less": "^4.1.2",
    "modern-copy": "^1.0.3",
    "moment": "^2.29.4",
    "postcss": "^8.4.32",
    "qr-code-styling": "^1.6.0-rc.1",
    "tailwindcss": "^3.4.0",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.3.13",
    "vue-i18n": "^9.5.0",
    "vue-router": "4",
    "vue-slicksort": "^2.0.0-alpha.5"
  },
  "devDependencies": {
    "@genql/cli": "^6.2.1",
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-vue": "^4.5.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vue-tsc": "^1.8.26"
  }
}
